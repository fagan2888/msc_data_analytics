library(readr)
library(magrittr)
library(tidyr)
library(dplyr)
select <- dplyr::select
library(lubridate)
library(stringr)
library(countreg)
library(DataCombine)
# library(corrplot)
# Load data -------------------------------------------------------------------------
ingredients <- read.csv('./data/ingredients.csv')
menu_items <- read.csv('./data/menu_items.csv')
menuitem <- read.csv('./data/menuitem.csv')
portion_uom_types <- read.csv('./data/portion_uom_types.csv')
pos_ordersale <- read.csv('./data/pos_ordersale.csv')
recipe_ingredient_assignments <- read.csv('./data/recipe_ingredient_assignments.csv')
recipe_sub_recipe_assignments <- read.csv('./data/recipe_sub_recipe_assignments.csv')
recipes <- read.csv('./data/recipes.csv')
store_restaurant <- read.csv('./data/store_restaurant.csv')
sub_recipe_ingr_assignments <- read.csv('./data/sub_recipe_ingr_assignments.csv')
sub_recipes <- read.csv('./data/sub_recipes.csv')
# Join Recipe to Ingredients to get Sales ---------------------------------------------
recipe <- menuitem %>%
left_join(menu_items, by = c("Id"="MenuItemId")) %>%
left_join(recipe_ingredient_assignments, by = c("RecipeId"="RecipeId")) %>%
filter(IngredientId %in% c(27,28)) %>%
mutate(sales = Quantity.x * Quantity.y) %>%
group_by(StoreNumber, date, IngredientId) %>%
summarise(demand = sum(sales))
# Join Sub-Recipes to Ingredients to get Sales -----------------------------------------
subrecipe <- menuitem %>%
left_join(menu_items, by = c("Id"="MenuItemId")) %>%
left_join(recipe_sub_recipe_assignments, by = c("RecipeId" = "RecipeId")) %>%
na.omit() %>%
left_join(sub_recipe_ingr_assignments, by = c("SubRecipeId" = "SubRecipeId")) %>%
filter (IngredientId %in% c(27,28)) %>%
mutate(sales = Factor * Quantity.x * Quantity.y) %>%
group_by(StoreNumber, date, IngredientId) %>%
summarise(demand = sum(sales))
# Make One View of All Ingredients -----------------------------------------------------
all_recipes <- rbind(recipe, subrecipe) %>%
group_by(StoreNumber, date, IngredientId) %>%
summarise(total_ingr_demand = sum(demand))
ingredient_demand <- all_recipes %>%
mutate(day = c(1, 103)) %>%
group_by(StoreNumber, day) %>%
summarise(tomato = sum(ifelse(IngredientId == '28', total_ingr_demand, 0)),
